{
  "name": "irewoledeasooke",
  "version": "1.0.0",
  "description": "A fullstack web application for Irewolede Asooke (A fashion niche brand)",
  "main": "server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "serve": "npm run build && node dist/server.js",
    "serve-dev": "npm run build && nodemon dist/server.js",
    "start": "npm run serve",
    "start-dev": "npm run serve-dev",
    "update": "npm run check-updates && npm run updates-install",
    "check-updates": "npx npm-check-updates -u",
    "updates-install": "npm install",
    "remove-updates": "rm -Rf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gentcod/FashionNiche.git"
  },
